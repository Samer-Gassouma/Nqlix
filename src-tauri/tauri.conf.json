{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Nqlix",
    "version": "0.0.11"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true,
        "scope": ["http://**", "https://**"]
      }
    },
    "bundle": {
      "active": true,
      "targets": ["msi", "nsis", "updater", "appimage"],
      "identifier": "com.nqlix.desktop",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://tunimove.vercel.app/api/updater/nqlix"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDE3NjQ1QzgxQUIzQTg3OEMKUldTTWh6cXJnVnhrRnkrWXVMR0R2S0hqQnY5UUxYODh6ZzJlRlpuRG0rN05LMHliZk1pQit1UVYK"
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline';"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Nqlix",
        "height": 600,
        "width": 800
      }
    ]
  }
}